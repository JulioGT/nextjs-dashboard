# Environment Variables
# Copy this file to .env.local and fill in the values

# Database - Supabase PostgreSQL
POSTGRES_URL=your_supabase_postgres_url_here
POSTGRES_PRISMA_URL=your_supabase_prisma_url_here
POSTGRES_URL_NON_POOLING=your_supabase_direct_url_here
POSTGRES_USER=postgres
POSTGRES_HOST=your_supabase_host_here
POSTGRES_PASSWORD=your_supabase_password_here
POSTGRES_DATABASE=postgres

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_SECRET_KEY=your_supabase_secret_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# NextAuth - Required for authentication
NEXTAUTH_SECRET=your_secret_key_here
NEXTAUTH_URL=http://localhost:3000

# Optional: If using custom auth URL
AUTH_URL=http://localhost:3000
AUTH_SECRET=your_secret_key_here